{
  "name": "auto-pr-creator",
  "version": "1.0.0",
  "description": "Automatically create pull requests that change CSV files",
  "author": "Tadao Iseki <saitoeku3@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start": "cross-env NODE_ENV=production forever start ./.nuxt/server.js",
    "stop": "forever stopall"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.2",
    "@nuxt/vue-app": "^2.12.2",
    "@nuxtjs/axios": "^5.9.7",
    "@octokit/rest": "^17.1.4",
    "atob": "^2.1.2",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "csv-parse": "^4.8.8",
    "express": "^4.16.4",
    "http-errors": "^1.7.3",
    "nuxt": "^2.12.2",
    "ts-node": "^8.8.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.2",
    "@types/atob": "^2.1.2",
    "@types/http-errors": "^1.6.3",
    "@types/sax": "^1.2.1",
    "@types/uuid": "^7.0.2",
    "forever": "^2.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "sass-loader": "^8.0.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  }
}
